{
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
        "Url": "http://localhost:5001",
        "Protocols": "Http2"
      }
    }
  },
  "AyBorg": {
    "Service": {
      "DisplayName": "AyBorg.Analytics",
      "UniqueName": "AyBorg.Analytics",
      "Type": "Analytics",
      "Url": "http://localhost:5001",
      "Audit": {
        "Required": false
      }
    },
    "Gateway": {
      "URL": "http://localhost:5000"
    },
    "EventStorage": {
      "MaxDaysToKeep": 30
    }
  },
  "ConnectionStrings": {
    "Database": "Filename=eventLog.db;Flush=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System.Net": "Warning"
    }
  },
  "AllowedHosts": "*"
}
