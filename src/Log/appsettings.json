{
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
        "Url": "http://localhost:6001",
        "Protocols": "Http2"
      }
    }
  },
  "AyBorg": {
    "Service": {
      "DisplayName": "AyBorg.Log",
      "UniqueName": "AyBorg.Log",
      "Type": "Log",
      "Url": "http://localhost:6001",
      "Audit": {
        "Required": false
      }
    },
    "Gateway": {
      "Url": "http://localhost:6000"
    },
    "EventStorage": {
      "MaxDaysToKeep": 30
    }
  },
  "ConnectionStrings": {
    "Database": "Filename=eventLog.db;Flush=true"
  },
  "OpenTelemetry":
  {
    "Enabled": true
  },
  "ElasticApm": {
    "Enabled": false,
    "ServerUrl":  "http://localhost:8200",
    "SecretToken":  "",
    "TransactionSampleRate": 1.0,
    "LogLevel": "Information"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System.Net": "Warning"
    },
    "Elasticsearch": {
      "ShipTo": {
        "NodePoolType": "SingleNode",
        "NodeUris": [ ],
        "ApiKey": "",
        "Username": "",
        "Password": ""
      }
    }
  },
  "AllowedHosts": "*"
}
