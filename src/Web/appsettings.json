{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:6010"
      },
      "Https": {
        "Url": "https://localhost:6011"
      }
    }
  },
  "AyBorg": {
    "Service": {
      "DisplayName": "AyBorg.Web",
      "UniqueName": "AyBorg.Web",
      "Type": "Web",
      "Url": "https://localhost:6011",
      "Audit": {
        "Required": false
      }
    },
    "Gateway": {
      "URL": "http://localhost:6000"
    },
    "Jwt": {
      "SecretKey": "_AyBorgSecretKey_"
    }
  },
  "DatabaseProvider": "SqlLite",
  "ConnectionStrings": {
    "SqlLiteConnection": "DataSource=app.db;Cache=Shared",
    "PostgreSqlConnection": "Host=localhost;Database=AyBorg;Username=postgres;Password=postgres",
    "IdentityDataContextConnection": "Data Source=AyBorg.Web.db"
  },
  "OpenTelemetry":
  {
    "Enabled": true
  },
  "ElasticApm": {
    "Enabled": true,
    "ServerUrl":  "http://localhost:8200",
    "SecretToken":  "",
    "TransactionSampleRate": 1.0,
    "LogLevel": "Information"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System.Net": "Warning"
    },
    "Elasticsearch": {
      "ShipTo": {
        "NodePoolType": "SingleNode",
        "NodeUris": [ ],
        "ApiKey": "",
        "Username": "",
        "Password": ""
      }
    }
  },
  "AllowedHosts": "*"
}
