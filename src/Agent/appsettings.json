{
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
        "Url": "http://localhost:5020",
        "Protocols": "Http2"
      }
    }
  },
  "DatabaseProvider": "SqlLite",
  "ConnectionStrings": {
    "SqlLiteConnection": "DataSource=app.db;Cache=Shared",
    "PostgreSqlConnection": "Host=localhost;Database=AyBorg;Username=postgres;Password=postgres"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System.Net": "Warning"
    }
  },
  "MQTT": {
    "Host": "localhost",
    "Port": 1883,
    "SecurePort": 8883
  },
  "AyBorg": {
    "Service": {
      "DisplayName": "AyBorg.Agent",
      "UniqueName": "AyBorg.Agent-1",
      "Type": "AyBorg.Agent",
      "Url": "http://localhost:5020"
    },
    "Cache": {
      "MaxSeconds": 10,
      "MaxIterations": 3
    },
    "Plugins": {
      "Folder": "/app/plugins"
    },
    "Gateway": {
      "URL": "http://localhost:5000"
    },
    "Jwt": {
      "SecretKey": "_AyBorgSecretKey_"
    }
  },
  "AllowedHosts": "*"
}
